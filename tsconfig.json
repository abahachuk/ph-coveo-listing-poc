{
 "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "skipLibCheck": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "module": "Node16",
    "moduleResolution": "Node16",
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": ["src"]
}


